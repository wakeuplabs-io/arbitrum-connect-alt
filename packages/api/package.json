{
  "name": "@arbitrum-connect/api",
  "version": "1.0.0",
  "description": "Template for Hono API",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "clean": "rm -rf dist",
    "build:types": "tsc",
    "build:bundle": "esbuild src/handler.ts --bundle --platform=node --format=cjs --outfile=dist/bundle.js",
    "build": "npm run clean && npm run build:types && npm run build:bundle",
    "start": "node dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "esbuild": "^0.21.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@arbitrum-connect/utils": "file:../utils",
    "@arbitrum-connect/db": "file:../db",
    "@hono/node-server": "^1.8.0",
    "@hono/swagger-ui": "^0.5.1",
    "@hono/zod-openapi": "^0.19.2",
    "@hono/zod-validator": "^0.4.3",
    "@scalar/hono-api-reference": "^0.7.1",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^12.0.1",
    "drizzle-orm": "0.28.5",
    "ethers": "5.8.0",
    "hono-openapi": "^0.4.6",
    "hono-pino": "^0.8.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "stoker": "^1.4.2",
    "zod": "^3.23.8",
    "zod-openapi": "^4.2.3"
  }
}
